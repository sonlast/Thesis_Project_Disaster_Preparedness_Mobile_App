{
  "expo": {
    "name": "Quick Response Aid",
    "slug": "Qat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/app-icons-xplatform/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/app-icons-xplatform/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/app-icons-xplatform/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.i_son.lastimosa.Qat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "183b9b74-b620-45cb-aba3-262d1940aa0d"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ${PRODUCT NAME} to access your location"
        }
      ]
    ]
  }
}